# Selenium 테스트 환경 설정

# 기본 URL
BASE_URL=https://www.google.com

# 브라우저 설정
BROWSER=chrome
HEADLESS=false

# 타임아웃 설정 (초)
IMPLICIT_WAIT=10
EXPLICIT_WAIT=20
PAGE_LOAD_TIMEOUT=30

# 윈도우 크기
WINDOW_WIDTH=1920
WINDOW_HEIGHT=1080

# 스크린샷 디렉토리
SCREENSHOT_DIR=reports/screenshots

# 테스트 데이터
TEST_USERNAME=test_user
TEST_PASSWORD=test_password
